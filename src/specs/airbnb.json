		"http_method" : "GET",
		"parameters":[
			{
				"name": "id",
				"in": "path",
				"demanded": true
			},
			{
				"name":"client_id", 
				"in": "query_string",
				"client_id": "3092nxybyb0otqw18e8nh5nty"
			}
		],
		"after": [
			{
				"token_save": false,
				"user": "picture_url",
				"user": "first_name"
			}
		]
	},
	"login": {
		"url" : "v2/login/",
