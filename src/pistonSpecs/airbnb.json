{
  "info": {
    "title": "AirBnB",
    "description": "API to retrieve different info from AirBnB",
    "version": "1.0",
    "author": "h4ckademy"
  },
  "defaults": {
    "baseUrl": "https://api.airbnb.com"
  },
  "action": [{
    "name": "get_user",
    "uri": "https://api.airbnb.com/v2/users/",
    "qs": {
      "client_id": "3092nxybyb0otqw18e8nh5nty"
    },
    "method": "GET",
    "headers": {
      "host": "api.airbnb.com",
      "User-Agent": "Airbnb/160610 Android/4.0.4 Device/Sony_LT15i Carrier/Movistar Type/Phone",
      "Connection": "keep-alive"
    },
    "after": null,
    "extract": [
      "user.first_name",
      "user.picture_url"
    ]
  }, {
    "name": "login",
    "uri": "https://api.airbnb.com/v1/authorize",
    "qs": {
      "client_id": "3092nxybyb0otqw18e8nh5nty"
    },
    "method": "POST",
    "headers": {
      "User-Agent": "Airbnb/160610 Android/4.0.4 Device/Sony_LT15i Carrier/Movistar Type/Phone"
    },
    "form": {
      "grant_type": "password",
      "password": "123456",
      "user": "i1264348@trbvm.com"
      },
    "after": null,
    "extract": [
      "access_token"
    ]
  }]
}
